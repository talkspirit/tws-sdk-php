{
    "operations": {
        "getUsers": {
            "httpMethod": "GET",
            "uri": "users/search",
            "summary": "Gets a list of users",
            "responseClass": "GetUsersOutput"
        },
        "getMe": {
            "httpMethod": "GET",
            "uri": "users/default",
            "summary": "Retrieves his profile",
            "responseClass": "GetUserOutput"
        },
        "postUser": {
            "httpMethod": "POST",
            "uri": "users/default",
            "summary": "Creates a new user",
            "responseClass": "GetUserOutput",
            "parameters": {
                "user": {
                    "location": "json",
                    "type": "array"
                }
            }
        },
        "getUser": {
            "httpMethod": "GET",
            "uri": "users/{id}",
            "summary": "Retrieves a single user",
            "responseClass": "GetUserOutput",
            "parameters": {
                "id": {
                    "location": "uri",
                    "description": "User to retrieve by ID",
                    "required": true
                }
            }
        },
        "deleteUser": {
            "httpMethod": "DELETE",
            "uri": "users/{id}",
            "summary": "Deletes a user",
            "responseClass": "DeleteUserOutput",
            "parameters": {
                "id": {
                    "location": "uri",
                    "description": "User to delete by ID",
                    "required": true
                }
            }
        }
    },
    "models": {
        "GetUsersOutput": {
            "type": "object",
            "properties": {
                "users": {
                    "location": "json",
                    "type": "string"
                }
            }
        },
        "GetUserOutput": {
            "type": "object",
            "properties": {
                "user": {
                    "location": "json",
                    "type": "string"
                },
                "statusCode": {
                    "location": "statusCode",
                    "type": "integer"
                }
            }

        },
        "DeleteUserOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "statusCode",
                    "type": "integer"
                }
            }
        }
    }
}